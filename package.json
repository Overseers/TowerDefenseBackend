{
    "name": "TowerDefense",
    "version": "1.1.0",
    "description": "",
    "scripts": {
        "test": "jest --collectCoverage",
        "clean": "(rimraf ./dev/ && rimraf ./dist/ && rimraf ./package-lock.json && rimraf ./node_modules/ && npm i) || npm i",
        "build:dev": "babel ./src/ --delete-dir-on-start -d ./dist/ && node ./dist/index",
        "build:prod": "babel ./src/ --delete-dir-on-start --compact true --minified -d ./dist/ && node ./dist/index"
    },
    "author": "Overseers",
    "license": "ISC",
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "dependencies": {
        "cookie-parser": "^1.4.4",
        "express": "^4.17.1",
        "node-fetch": "^2.6.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.7",
        "@babel/preset-env": "^7.8.7",
        "jest": "^25.1.0",
        "rimraf": "^3.0.2"
    }
}
